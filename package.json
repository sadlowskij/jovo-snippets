{
    "name": "jovo-snippets",
    "displayName": "Jovo Snippets",
    "description": "Handy snippets for Jovo v4",
    "version": "0.0.23",
    "publisher": "jsadlowski",
    "icon": "images/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/sadlowskij/jovo-snippets.git"
    },
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "typescript",
                "path": "./snippets/typescript.code-snippets"
            },
            {
                "language": "json",
                "path": "./snippets/json.code-snippets"
            },
            {
                "language": "javascript",
                "path": "./snippets/javascript.code-snippets"
            }
        ]
    },
    "devDependencies": {
        "patch-package": "^6.4.7",
        "vsce": "^2.11.0"
    }
}
